{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@imgs/*": [
        "src/assets/images/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-jsx",
    "verbatimModuleSyntax": true,
    "rootDirs": [
      "./src/components/react",
      "./src/components/solid",
    ],
  }
}